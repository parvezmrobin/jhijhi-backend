  "description": "A simple react app backed by Express JS to maintain cricket score",
  "homepage": "https://jhijhi.herokuapp.com",
  "keywords": [
    "crikect",
    "scoring",
    "react",
    "express",
    "jhijhi",
    "custom-scoring",
    "cricket-scoring"
  ],
  "repository": {
    "type": "git",
